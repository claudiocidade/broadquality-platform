<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="webDriverManager" class="main.java.com.framework.configuration.model.serialization.WebDriverManager" >
        <constructor-arg value="CHROME" type="main.java.com.framework.test.selenium.DriverFactory.DriverType" />
    </bean>
</beans>